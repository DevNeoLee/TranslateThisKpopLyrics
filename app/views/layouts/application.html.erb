<!DOCTYPE html>
<html>
  <head>
    <title>NewBlog11KpopGroupAlbumSongTranslationComment</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <% if user_signed_in? %>
      <%= link_to "Logout", destroy_user_session_path , method: :delete %> |
      <%= link_to "Profile Edit", edit_user_registration_path %>
    <% else %>
      <%= link_to "Login", new_user_session_path %> | 
      <%= link_to "Signup", new_user_registration_path %> |
      <%= link_to "Login with Facebook", user_facebook_omniauth_authorize_path %> |
      <%= link_to "Login with Google", user_google_oauth2_omniauth_authorize_path %>
    <% end %>
    <br>
    <br>
    <%= link_to "Logo", groups_index_path %>
    <br>
    <p class="notice"><%= notice %></p>
    <p class="alert"><%= alert %></p>


    <%= yield %>
  </body>
</html>
